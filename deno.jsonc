{
  "name": "@lab51-tools/artistic-data-visualizer",
  "version": "1.0.0-rc.2",
  "exports": {
    ".": "./mod.js"
  },
  "author": "lab51-tools",
  "license": "MIT",
  "lint": {
    "include": ["src/"],
    "rules": {
      "exclude": []
    }
  },
  "fmt": {
    "singleQuote": true,
    "semiColons": false,
    "lineWidth": 200,
    "proseWrap": "preserve",
    "include": ["src/"]
  },
  "publish": {
    "exclude": [
