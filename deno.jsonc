  "tasks": {
    "lint": "deno lint && deno fmt --check",
    "test": "deno test --allow-env=DEBUG --allow-read=./ --allow-write=./",
    "publish:dry": "deno publish --dry-run"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.0",
    "lodash-es": "npm:lodash-es@^4.17.21",
    "pngjs": "npm:pngjs@^7.0.0"
  }
}